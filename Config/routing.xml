<?xml version="1.0" encoding="UTF-8"?>
<routes xmlns="http://symfony.com/schema/routing" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="stockonorder.config.edit_module" path="/admin/module/StockOnOrder/viewModule/{id}" methods="get">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:viewModule</default>
        <requirement key="id">\d+</requirement>
    </route>

    <route id="stockonorder.stock_on_order_config.list" path="/admin/module/StockOnOrder/stock_on_order_config" methods="get">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:default</default>
    </route>
    <route id="stockonorder.stock_on_order_config.create" path="/admin/module/StockOnOrder/stock_on_order_config" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:create</default>
    </route>
    <route id="stockonorder.stock_on_order_config.view" path="/admin/module/StockOnOrder/stock_on_order_config/edit" methods="get">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:update</default>
    </route>
    <route id="stockonorder.stock_on_order_config.edit" path="/admin/module/StockOnOrder/stock_on_order_config/edit" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:processUpdate</default>
    </route>
    <route id="stockonorder.stock_on_order_config.delete" path="/admin/module/StockOnOrder/stock_on_order_config/delete" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrderConfig:delete</default>
    </route>
    <route id="stockonorder.stock_on_order.list" path="/admin/module/StockOnOrder/stock_on_order" methods="get">
        <default key="_controller">StockOnOrder:StockOnOrder:default</default>
    </route>
    <route id="stockonorder.stock_on_order.create" path="/admin/module/StockOnOrder/stock_on_order" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrder:create</default>
    </route>
    <route id="stockonorder.stock_on_order.view" path="/admin/module/StockOnOrder/stock_on_order/edit" methods="get">
        <default key="_controller">StockOnOrder:StockOnOrder:update</default>
    </route>
    <route id="stockonorder.stock_on_order.edit" path="/admin/module/StockOnOrder/stock_on_order/edit" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrder:processUpdate</default>
    </route>
    <route id="stockonorder.stock_on_order.delete" path="/admin/module/StockOnOrder/stock_on_order/delete" methods="post">
        <default key="_controller">StockOnOrder:StockOnOrder:delete</default>
    </route>

</routes>
